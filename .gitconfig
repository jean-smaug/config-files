[core]
	editor = vim
[alias]
	config = config --global --edit
	co = checkout
	cob = checkout -b
	cm = commit -m
	cam = commit -am
	amend = commit --amend
	romi = rebase origin/master -i
	remaster = pull origin/master && reha origin/master
	st = status
	reha = reset --hard
	aa = add .
	ap = add -p
	pou = push -u origin
	pouf = push -f

